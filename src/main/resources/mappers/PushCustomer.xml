<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fjs.cronus.mappers.PushCustomerMapper" >



    <insert id="addPushCustomer" parameterType="com.fjs.cronus.entity.PushCustomerEntity">
        INSERT INTO push_customer (customer_info_id,sub_company_id,sub_company_name)
